#include<stdio.h>
#include<conio.h>
main()
{
    int a,b;
    printf("Enter the value of a:\n");
    scanf("%d",&a);
    printf("Enter the value of b:\n");
    scanf("%d",&b);
    {
        if(a==b)
        printf("The number is equal");
        else
        printf("The number is not equal");
    }
    getch();
}
